   <link target="#pers00463" type="author"/>
   <link target="#nott" type="nott"/>
   <note type="source" xml:id="pftm-n00485">
      <date notAfter="1883" type="comp">
         <w xml:id="pftm-125700">1883</w>
      </date>
   </note>
   <head type="main" xml:lang="pol" xml:id="pftm-h00380">
      <w xml:id="pftm-124950">Sfinks</w>
   </head>
   <lg xml:id="pftm-s00930">
      <l xml:id="pftm-l06720">
         <w xml:id="pftm-124980">Zastąpił</w>
         <c> </c>
         <w xml:id="pftm-124990">mi</w>
         <c> </c>
         <w xml:id="pftm-125000">raz</w>
         <c> </c>
         <w xml:id="pftm-125010">Sfinks</w>
         <c> </c>
         <w xml:id="pftm-125020">u</w>
         <c> </c>
         <w xml:id="pftm-125030">ciemnej</w>
         <c> </c>
         <w xml:id="pftm-125040">skały</w>
         <pc xml:id="pftm-125050">,</pc>
      </l>
      <l xml:id="pftm-l06725">
         <c> </c>
         <w xml:id="pftm-125060">Gdzie</w>
         <pc xml:id="pftm-125070">,</pc>
         <c> </c>
         <w xml:id="pftm-125080">jak</w>
         <c> </c>
         <w xml:id="pftm-125090">zbójca</w>
         <pc xml:id="pftm-125100">,</pc>
         <c> </c>
         <w xml:id="pftm-125110">celnik</w>
         <pc xml:id="pftm-125120">,</pc>
         <c> </c>
         <w xml:id="pftm-125130">lub</w>
         <c> </c>
         <w xml:id="pftm-125140">człowiek</w>
         <c> </c>
         <w xml:id="pftm-125150">biedny</w>
         <pc xml:id="pftm-125160">,</pc>
      </l>
      <l xml:id="pftm-l06730">
         <c> </c>
         <pc xml:id="pftm-125170">«</pc>
         <c> </c>
         <w xml:id="pftm-125180">Prawd</w>
         <pc xml:id="pftm-125190">!</pc>
         <c> </c>
         <pc xml:id="pftm-125200">»</pc>
         <c> </c>
         <w xml:id="pftm-125210">wołając</w>
         <pc xml:id="pftm-125220">,</pc>
         <c> </c>
         <w xml:id="pftm-125230">wciąż</w>
         <c> </c>
         <w xml:id="pftm-125240">prawd</w>
         <c> </c>
         <w xml:id="pftm-125250">zgłodniały</w>
         <pc xml:id="pftm-125260">,</pc>
      </l>
      <l xml:id="pftm-l06735">
         <c> </c>
         <w xml:id="pftm-125270">Nie</w>
         <c> </c>
         <w xml:id="pftm-125280">dawa</w>
         <c> </c>
         <w xml:id="pftm-125290">gościom</w>
         <c> </c>
         <w xml:id="pftm-125300">tchu</w>
         <pc xml:id="pftm-125310" unit="sentence">.</pc>
         <c> </c>
      </l>
   </lg>
   <lg xml:id="pftm-s00935">
      <l xml:id="pftm-l06740">
         <pc xml:id="pftm-125340">«</pc>
         <c> </c>
         <w xml:id="pftm-125350">Człowiek</w>
         <pc xml:id="pftm-125360">?</pc>
         <c> </c>
         <pc xml:id="pftm-125370" unit="sentence">...</pc>
         <c> </c>
         <w xml:id="pftm-125380">Jest</w>
         <c> </c>
         <w xml:id="pftm-125390">to</w>
         <c> </c>
         <w xml:id="pftm-125400">kapłan</w>
         <c> </c>
         <w xml:id="pftm-125410">bezwiedny</w>
      </l>
      <l xml:id="pftm-l06745">
         <c> </c>
         <w xml:id="pftm-125420">I</w>
         <c> </c>
         <w xml:id="pftm-125430">niedojrzały</w>
         <c> </c>
         <pc xml:id="pftm-125440">...</pc>
         <c> </c>
         <pc xml:id="pftm-125450" unit="sentence">»</pc>
         <c> </c>
      </l>
      <l xml:id="pftm-l06750">
         <w xml:id="pftm-125460">Odpowiedziałem</w>
         <c> </c>
         <w xml:id="pftm-125470">mu</w>
         <pc xml:id="pftm-125480" unit="sentence">.</pc>
         <c> </c>
      </l>
   </lg>
   <lg xml:id="pftm-s00940">
      <l xml:id="pftm-l06755">
         <w xml:id="pftm-125510">Alić</w>
         <c> </c>
         <pc xml:id="pftm-125520">—</pc>
         <c> </c>
         <w xml:id="pftm-125530">o</w>
         <c> </c>
         <w xml:id="pftm-125540">dziwy</w>
         <pc xml:id="pftm-125550">!</pc>
         <c> </c>
         <pc xml:id="pftm-125560">—</pc>
      </l>
      <l xml:id="pftm-l06760">
         <c> </c>
         <w xml:id="pftm-125570">Sfinks</w>
         <c> </c>
         <w xml:id="pftm-125580">się</w>
         <c> </c>
         <w xml:id="pftm-125590">cofnął</w>
         <c> </c>
         <w xml:id="pftm-125600">grzbietem</w>
         <c> </c>
         <w xml:id="pftm-125610">do</w>
         <c> </c>
         <w xml:id="pftm-125620">skały</w>
         <c> </c>
         <pc xml:id="pftm-125630" unit="sentence">...</pc>
         <c> </c>
      </l>
      <l xml:id="pftm-l06765">
         <w xml:id="pftm-125640">Przemknąłem</w>
         <c> </c>
         <w xml:id="pftm-125650">żywy</w>
         <pc xml:id="pftm-125660" unit="sentence">!</pc>
         <c> </c>
      </l>
   </lg>
</div>
