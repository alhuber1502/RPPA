     xml:id="text01308"
     xml:lang="rus">
   <link target="#pers00238" type="author"/>
   <link target="#nott" type="nott"/>
   <note type="source" xml:id="erpa-n01720">
      <date cert="unknown" type="comp" when="1836">
         <w xml:id="erpa-1512380">1836</w>
         <pc xml:id="erpa-1512390" unit="sentence">?</pc>
      </date>
   </note>
   <head type="main" xml:lang="rus" xml:id="erpa-h02345">
      <w xml:id="erpa-1511430">Сумерки</w>
   </head>
   <lg xml:id="erpa-s11815">
      <l xml:id="erpa-l84060">
         <w xml:id="erpa-1511460">‎</w>
         <c> </c>
         <w xml:id="erpa-1511470">Тѣни</w>
         <c> </c>
         <w xml:id="erpa-1511480">сизыя</w>
         <c> </c>
         <w xml:id="erpa-1511490">смѣсились</w>
         <pc xml:id="erpa-1511500">,</pc>
      </l>
      <l xml:id="erpa-l84065">
         <w xml:id="erpa-1511510">Цвѣтъ</w>
         <c> </c>
         <w xml:id="erpa-1511520">поблекнулъ</w>
         <pc xml:id="erpa-1511530">,</pc>
         <c> </c>
         <w xml:id="erpa-1511540">звукъ</w>
         <c> </c>
         <w xml:id="erpa-1511550">уснулъ</w>
         <pc xml:id="erpa-1511560">;</pc>
      </l>
      <l xml:id="erpa-l84070">
         <w xml:id="erpa-1511570">Жизнь</w>
         <pc xml:id="erpa-1511580">,</pc>
         <c> </c>
         <w xml:id="erpa-1511590">движенье</w>
         <c> </c>
         <w xml:id="erpa-1511600">разрѣшились</w>
      </l>
      <l xml:id="erpa-l84075">
         <w xml:id="erpa-1511610">Въ</w>
         <c> </c>
         <w xml:id="erpa-1511620">сумракъ</w>
         <c> </c>
         <w xml:id="erpa-1511630">зыбкій</w>
         <pc xml:id="erpa-1511640">,</pc>
         <c> </c>
         <w xml:id="erpa-1511650">въ</w>
         <c> </c>
         <w xml:id="erpa-1511660">дальній</w>
         <c> </c>
         <w xml:id="erpa-1511670">гулъ</w>
         <c> </c>
         <pc xml:id="erpa-1511680">…</pc>
      </l>
      <l xml:id="erpa-l84080">
         <w xml:id="erpa-1511690">Мотылька</w>
         <c> </c>
         <w xml:id="erpa-1511700">полетъ</w>
         <c> </c>
         <w xml:id="erpa-1511710">незримый</w>
      </l>
      <l xml:id="erpa-l84085">
         <w xml:id="erpa-1511720">Слышенъ</w>
         <c> </c>
         <w xml:id="erpa-1511730">въ</w>
         <c> </c>
         <w xml:id="erpa-1511740">воздухѣ</w>
         <c> </c>
         <w xml:id="erpa-1511750">ночномъ</w>
         <c> </c>
         <pc xml:id="erpa-1511760">…</pc>
      </l>
      <l xml:id="erpa-l84090">
         <w xml:id="erpa-1511770">Часъ</w>
         <c> </c>
         <w xml:id="erpa-1511780">тоски</w>
         <c> </c>
         <w xml:id="erpa-1511790">невыразимой</w>
         <pc xml:id="erpa-1511800" unit="sentence">!</pc>
      </l>
      <l xml:id="erpa-l84095">
         <w xml:id="erpa-1511810">Все</w>
         <c> </c>
         <w xml:id="erpa-1511820">во</w>
         <c> </c>
         <w xml:id="erpa-1511830">мнѣ</w>
         <pc xml:id="erpa-1511840">,</pc>
         <c> </c>
         <pc xml:id="erpa-1511850">—</pc>
         <c> </c>
         <w xml:id="erpa-1511860">и</w>
         <c> </c>
         <w xml:id="erpa-1511870">я</w>
         <c> </c>
         <w xml:id="erpa-1511880">во</w>
         <c> </c>
         <w xml:id="erpa-1511890">всемъ</w>
         <c> </c>
         <pc xml:id="erpa-1511900">…</pc>
      </l>
   </lg>
   <lg xml:id="erpa-s11820">
      <l xml:id="erpa-l84100">
         <w xml:id="erpa-1511930">‎</w>
         <c> </c>
         <w xml:id="erpa-1511940">Сумракъ</w>
         <c> </c>
         <w xml:id="erpa-1511950">тихій</w>
         <pc xml:id="erpa-1511960">,</pc>
         <c> </c>
         <w xml:id="erpa-1511970">сумракъ</w>
         <c> </c>
         <w xml:id="erpa-1511980">сонный</w>
         <pc xml:id="erpa-1511990">,</pc>
      </l>
      <l xml:id="erpa-l84105">
         <w xml:id="erpa-1512000">Лейся</w>
         <c> </c>
         <w xml:id="erpa-1512010">въ</w>
         <c> </c>
         <w xml:id="erpa-1512020">глубь</w>
         <c> </c>
         <w xml:id="erpa-1512030">моей</w>
         <c> </c>
         <w xml:id="erpa-1512040">души</w>
         <pc xml:id="erpa-1512050">,</pc>
      </l>
      <l xml:id="erpa-l84110">
         <w xml:id="erpa-1512060">Тихій</w>
         <pc xml:id="erpa-1512070">,</pc>
         <c> </c>
         <w xml:id="erpa-1512080">томный</w>
         <pc xml:id="erpa-1512090">,</pc>
         <c> </c>
         <w xml:id="erpa-1512100">благовонный</w>
         <pc xml:id="erpa-1512110">,</pc>
      </l>
      <l xml:id="erpa-l84115">
         <w xml:id="erpa-1512120">Все</w>
         <c> </c>
         <w xml:id="erpa-1512130">залей</w>
         <c> </c>
         <w xml:id="erpa-1512140">и</w>
         <c> </c>
         <w xml:id="erpa-1512150">утиши</w>
         <pc xml:id="erpa-1512160" unit="sentence">.</pc>
      </l>
      <l xml:id="erpa-l84120">
         <w xml:id="erpa-1512170">Чувства</w>
         <c> </c>
         <pc xml:id="erpa-1512180">—</pc>
         <c> </c>
         <w xml:id="erpa-1512190">мглой</w>
         <c> </c>
         <w xml:id="erpa-1512200">самозабвенья</w>
      </l>
      <l xml:id="erpa-l84125">
         <w xml:id="erpa-1512210">Переполни</w>
         <c> </c>
         <w xml:id="erpa-1512220">черезъ</w>
         <c> </c>
         <w xml:id="erpa-1512230">край</w>
         <pc xml:id="erpa-1512240">!</pc>
         <c> </c>
         <pc xml:id="erpa-1512250" unit="sentence">..</pc>
      </l>
      <l xml:id="erpa-l84130">
         <w xml:id="erpa-1512260">Дай</w>
         <c> </c>
         <w xml:id="erpa-1512270">вкусить</w>
         <c> </c>
         <w xml:id="erpa-1512280">уничтоженья</w>
         <pc xml:id="erpa-1512290">,</pc>
      </l>
      <l xml:id="erpa-l84135">
         <w xml:id="erpa-1512300">Съ</w>
         <c> </c>
         <w xml:id="erpa-1512310">міромъ</w>
         <c> </c>
         <w xml:id="erpa-1512320">дремлющимъ</w>
         <c> </c>
         <w xml:id="erpa-1512330">смѣшай</w>
         <pc xml:id="erpa-1512340" unit="sentence">!</pc>
      </l>
   </lg>
</div>
