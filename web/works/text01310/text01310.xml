   <lg xml:id="erpa-s11835">
      <l xml:id="erpa-l84220">
         <w xml:id="erpa-1513650">‎</w>
         <c> </c>
         <w xml:id="erpa-1513660">Есть</w>
         <c> </c>
         <w xml:id="erpa-1513670">въ</w>
         <c> </c>
         <w xml:id="erpa-1513680">осени</w>
         <c> </c>
         <w xml:id="erpa-1513690">первоначальной</w>
      </l>
      <l xml:id="erpa-l84225">
         <w xml:id="erpa-1513700">Короткая</w>
         <pc xml:id="erpa-1513710">,</pc>
         <c> </c>
         <w xml:id="erpa-1513720">но</w>
         <c> </c>
         <w xml:id="erpa-1513730">дивная</w>
         <c> </c>
         <w xml:id="erpa-1513740">пора</w>
         <pc xml:id="erpa-1513750">:</pc>
      </l>
      <l xml:id="erpa-l84230">
         <w xml:id="erpa-1513760">Прозрачный</w>
         <c> </c>
         <w xml:id="erpa-1513770">воздухъ</w>
         <pc xml:id="erpa-1513780">,</pc>
         <c> </c>
         <w xml:id="erpa-1513790">день</w>
         <c> </c>
         <w xml:id="erpa-1513800">хрустальный</w>
         <pc xml:id="erpa-1513810">,</pc>
      </l>
      <l xml:id="erpa-l84235">
         <w xml:id="erpa-1513820">И</w>
         <c> </c>
         <w xml:id="erpa-1513830">лучезарны</w>
         <c> </c>
         <w xml:id="erpa-1513840">вечера</w>
         <c> </c>
         <pc xml:id="erpa-1513850">…</pc>
      </l>
   </lg>
   <lg xml:id="erpa-s11840">
      <l xml:id="erpa-l84240">
         <w xml:id="erpa-1513880">‎</w>
         <c> </c>
         <w xml:id="erpa-1513890">Гдѣ</w>
         <c> </c>
         <w xml:id="erpa-1513900">бодрый</w>
         <c> </c>
         <w xml:id="erpa-1513910">серпъ</w>
         <c> </c>
         <w xml:id="erpa-1513920">гулялъ</w>
         <c> </c>
         <w xml:id="erpa-1513930">и</w>
         <c> </c>
         <w xml:id="erpa-1513940">падалъ</w>
         <c> </c>
         <w xml:id="erpa-1513950">колосъ</w>
         <pc xml:id="erpa-1513960">,</pc>
      </l>
      <l xml:id="erpa-l84245">
         <w xml:id="erpa-1513970">Теперь</w>
         <c> </c>
         <w xml:id="erpa-1513980">ужъ</w>
         <c> </c>
         <w xml:id="erpa-1513990">пусто</w>
         <c> </c>
         <w xml:id="erpa-1514000">все</w>
         <c> </c>
         <pc xml:id="erpa-1514010">—</pc>
         <c> </c>
         <w xml:id="erpa-1514020">просторъ</w>
         <c> </c>
         <w xml:id="erpa-1514030">вездѣ</w>
         <pc xml:id="erpa-1514040">;</pc>
      </l>
      <l xml:id="erpa-l84250">
         <w xml:id="erpa-1514050">Лишь</w>
         <c> </c>
         <w xml:id="erpa-1514060">паутины</w>
         <c> </c>
         <w xml:id="erpa-1514070">тонкій</w>
         <c> </c>
         <w xml:id="erpa-1514080">волосъ</w>
      </l>
      <l xml:id="erpa-l84255">
         <w xml:id="erpa-1514090">Блеститъ</w>
         <c> </c>
         <w xml:id="erpa-1514100">на</w>
         <c> </c>
         <w xml:id="erpa-1514110">праздной</w>
         <c> </c>
         <w xml:id="erpa-1514120">бороздѣ</w>
         <pc xml:id="erpa-1514130" unit="sentence">.</pc>
      </l>
   </lg>
   <lg xml:id="erpa-s11845">
      <l xml:id="erpa-l84260">
         <w xml:id="erpa-1514160">‎</w>
         <c> </c>
         <w xml:id="erpa-1514170">Пустѣетъ</w>
         <c> </c>
         <w xml:id="erpa-1514180">воздухъ</w>
         <pc xml:id="erpa-1514190">,</pc>
         <c> </c>
         <w xml:id="erpa-1514200">птицъ</w>
         <c> </c>
         <w xml:id="erpa-1514210">не</w>
         <c> </c>
         <w xml:id="erpa-1514220">слышно</w>
         <c> </c>
         <w xml:id="erpa-1514230">болѣ</w>
         <pc xml:id="erpa-1514240">;</pc>
      </l>
      <l xml:id="erpa-l84265">
         <w xml:id="erpa-1514250">Но</w>
         <c> </c>
         <w xml:id="erpa-1514260">далеко</w>
         <c> </c>
         <w xml:id="erpa-1514270">еще</w>
         <c> </c>
         <w xml:id="erpa-1514280">до</w>
         <c> </c>
         <w xml:id="erpa-1514290">первыхъ</w>
         <c> </c>
         <w xml:id="erpa-1514300">зимнихъ</w>
         <c> </c>
         <w xml:id="erpa-1514310">бурь</w>
         <pc xml:id="erpa-1514320">,</pc>
      </l>
      <l xml:id="erpa-l84270">
         <w xml:id="erpa-1514330">И</w>
         <c> </c>
         <w xml:id="erpa-1514340">льется</w>
         <c> </c>
         <w xml:id="erpa-1514350">чистая</w>
         <c> </c>
         <w xml:id="erpa-1514360">и</w>
         <c> </c>
         <w xml:id="erpa-1514370">теплая</w>
         <c> </c>
         <w xml:id="erpa-1514380">лазурь</w>
      </l>
      <l xml:id="erpa-l84275">
         <w xml:id="erpa-1514390">На</w>
         <c> </c>
         <w xml:id="erpa-1514400">отдыхающее</w>
         <c> </c>
         <w xml:id="erpa-1514410">поле</w>
         <pc xml:id="erpa-1514420" unit="sentence">.</pc>
      </l>
   </lg>
