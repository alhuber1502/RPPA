   <head type="main" xml:id="erar-h02060">
      <w xml:id="erar-4085130">Lacul</w>
   </head>
   <lg xml:id="erar-s55870">
      <l xml:id="erar-l55505">
         <w xml:id="erar-4085160">Lacul</w>
         <c> </c>
         <w xml:id="erar-4085170">codrilor albastru</w>
      </l>
      <l xml:id="erar-l55510">
         <c> </c>
         <w xml:id="erar-4085180">Nuferi galbeni</w>
         <c> </c>
         <w xml:id="erar-4085190">îl</w>
         <c> </c>
         <w xml:id="erar-4085200">încarcă</w>
         <pc xml:id="erar-4085210">;</pc>
      </l>
      <l xml:id="erar-l55515">
         <c> </c>
         <w xml:id="erar-4085220">Tresărind</w>
         <c> </c>
         <w xml:id="erar-4085230">în</w>
         <c> </c>
         <w xml:id="erar-4085240">cercuri albe</w>
      </l>
      <l xml:id="erar-l55520">
         <c> </c>
         <w xml:id="erar-4085250">El cutremură</w>
         <c> </c>
         <w xml:id="erar-4085260">o</w>
         <c> </c>
         <w xml:id="erar-4085270">barcă</w>
         <pc xml:id="erar-4085280" unit="sentence">.</pc>
         <c> </c>
      </l>
   </lg>
   <lg xml:id="erar-s55875">
      <l xml:id="erar-l55525">
         <w xml:id="erar-4085300">Şi eu</w>
         <c> </c>
         <w xml:id="erar-4085310">trec</w>
         <c> </c>
         <w xml:id="erar-4085320">de-a</w>
         <c> </c>
         <w xml:id="erar-4085330">lung de</w>
         <c> </c>
         <w xml:id="erar-4085340">maluri</w>
         <pc xml:id="erar-4085350">,</pc>
      </l>
      <l xml:id="erar-l55530">
         <c> </c>
         <w xml:id="erar-4085360">Parc-ascult</w>
         <c> </c>
         <w xml:id="erar-4085370">şi</w>
         <c> </c>
         <w xml:id="erar-4085380">parc-aştept</w>
      </l>
      <l xml:id="erar-l55535">
         <c> </c>
         <w xml:id="erar-4085390">Ea din</w>
         <c> </c>
         <w xml:id="erar-4085400">trestii</w>
         <c> </c>
         <w xml:id="erar-4085410">să răsară</w>
      </l>
      <l xml:id="erar-l55540">
         <c> </c>
         <w xml:id="erar-4085420">Şi să-mi</w>
         <c> </c>
         <w xml:id="erar-4085430">cadă</w>
         <c> </c>
         <w xml:id="erar-4085440">lin</w>
         <c> </c>
         <w xml:id="erar-4085450">pe piept</w>
         <pc xml:id="erar-4085460">;</pc>
      </l>
   </lg>
   <lg xml:id="erar-s55880">
      <l xml:id="erar-l55545">
         <w xml:id="erar-4085480">Să</w>
         <c> </c>
         <w xml:id="erar-4085490">sărim</w>
         <c> </c>
         <w xml:id="erar-4085500">în</w>
         <c> </c>
         <w xml:id="erar-4085510">luntrea</w>
         <c> </c>
         <w xml:id="erar-4085520">mică</w>
         <pc xml:id="erar-4085530">,</pc>
      </l>
      <l xml:id="erar-l55550">
         <c> </c>
         <w xml:id="erar-4085540">Îngânaţi</w>
         <c> </c>
         <w xml:id="erar-4085550">de</w>
         <c> </c>
         <w xml:id="erar-4085560">glas</w>
         <c> </c>
         <w xml:id="erar-4085570">de</w>
         <c> </c>
         <w xml:id="erar-4085580">ape</w>
         <pc xml:id="erar-4085590">,</pc>
      </l>
      <l xml:id="erar-l55555">
         <c> </c>
         <w xml:id="erar-4085600">Şi să scap din</w>
         <c> </c>
         <w xml:id="erar-4085610">mână</w>
         <c> </c>
         <w xml:id="erar-4085620">cârma</w>
         <pc xml:id="erar-4085630">,</pc>
      </l>
      <l xml:id="erar-l55560">
         <c> </c>
         <w xml:id="erar-4085640">Şi lopeţile</w>
         <c> </c>
         <w xml:id="erar-4085650">să-mi</w>
         <c> </c>
         <w xml:id="erar-4085660">scape</w>
         <pc xml:id="erar-4085670">;</pc>
      </l>
   </lg>
   <lg xml:id="erar-s55885">
      <l xml:id="erar-l55565">
         <w xml:id="erar-4085690">Să</w>
         <c> </c>
         <w xml:id="erar-4085700">plutim</w>
         <c> </c>
         <w xml:id="erar-4085710">cuprinşi</w>
         <c> </c>
         <w xml:id="erar-4085720">de</w>
         <c> </c>
         <w xml:id="erar-4085730">farmec</w>
      </l>
      <l xml:id="erar-l55570">
         <c> </c>
         <w xml:id="erar-4085740">Sub</w>
         <c> </c>
         <w xml:id="erar-4085750">lumina</w>
         <c> </c>
         <w xml:id="erar-4085760">blândei</w>
         <c> </c>
         <w xml:id="erar-4085770">lune</w>
         <c> </c>
         <pc xml:id="erar-4085780">–</pc>
      </l>
      <l xml:id="erar-l55575">
         <c> </c>
         <w xml:id="erar-4085790">Vântu-n</w>
         <c> </c>
         <w xml:id="erar-4085800">trestii</w>
         <c> </c>
         <w xml:id="erar-4085810">lin</w>
         <c> </c>
         <w xml:id="erar-4085820">foşnească</w>
         <pc xml:id="erar-4085830">,</pc>
      </l>
      <l xml:id="erar-l55580">
         <c> </c>
         <w xml:id="erar-4085840">Unduioasa</w>
         <c> </c>
         <w xml:id="erar-4085850">apă</w>
         <c> </c>
         <w xml:id="erar-4085860">sune</w>
         <pc xml:id="erar-4085870" unit="sentence">!</pc>
         <c> </c>
      </l>
   </lg>
   <lg xml:id="erar-s55890">
      <l xml:id="erar-l55585">
         <w xml:id="erar-4085890">Dar</w>
         <c> </c>
         <w xml:id="erar-4085900">nu vine</w>
         <c> </c>
         <pc xml:id="erar-4085910" unit="sentence">...</pc>
         <c> </c>
         <w xml:id="erar-4085920">Singuratic</w>
      </l>
      <l xml:id="erar-l55590">
         <c> </c>
         <w xml:id="erar-4085930">În zadar suspin</w>
         <c> </c>
         <w xml:id="erar-4085940">şi</w>
         <c> </c>
         <w xml:id="erar-4085950">sufăr</w>
      </l>
      <l xml:id="erar-l55595">
         <c> </c>
         <w xml:id="erar-4085960">Lângă</w>
         <c> </c>
         <w xml:id="erar-4085970">lacul</w>
         <c> </c>
         <w xml:id="erar-4085980">cel</w>
         <c> </c>
         <w xml:id="erar-4085990">albastru</w>
      </l>
      <l xml:id="erar-l55600">
         <c> </c>
         <w xml:id="erar-4086000">Încărcat cu flori</w>
         <c> </c>
         <w xml:id="erar-4086010">de</w>
         <c> </c>
         <w xml:id="erar-4086020">nufăr</w>
         <pc xml:id="erar-4086030" unit="sentence">.</pc>
         <c> </c>
      </l>
   </lg>
</div>
