   <head type="main" xml:lang="rus" xml:id="erpa-h02295">
      <w xml:id="erpa-1498510">К</w>
      <c> </c>
      <w xml:id="erpa-1498520">чему</w>
      <c> </c>
      <w xml:id="erpa-1498530">невольнику</w>
      <c> </c>
      <w xml:id="erpa-1498540">мечтания</w>
      <c> </c>
      <w xml:id="erpa-1498550">свободы</w>
      <pc xml:id="erpa-1498560">?</pc>
      <c> </c>
